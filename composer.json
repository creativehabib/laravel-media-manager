{
    "name": "creativehabib/media-manager",
    "description": "Laravel media manager like Botble CMS",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Habib\\MediaManager\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Habib\\MediaManager\\MediaManagerServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.2",
        "livewire/livewire": "^3.3"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "homepage": "https://github.com/creativehabib/laravel-media-manager",
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
